{
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false,
          "useBuiltIns": false
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      ["@babel/plugin-proposal-class-properties", { "loose": true }]
    ],
    "comments": false
  }
  